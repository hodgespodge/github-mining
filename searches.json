[
    {
        "name" : "behavior_trees",
        "keywords": [
            "ros",
            "ros2"
        ],
        "qualifiers":[
            "stars:>2", "language:python"
        ],

        "repo_targets" :{

            "x1" : ".*",
            "x2" : {

                "directory_targets": {

                    "x1": ".*test",
                    "x2": ".*tests",

                    "x3": {
                        "file_targets" : {
                            "x1" : "CMakeLists.txt",
                            "x2" : ".*.py",
                            "x3" : {

                                "code_targets" : {
                                    "x1" : "import py_trees",
                                    "x2" : "from py_trees",
                                    "equation" : "x1 or x2"
                                }
                            },

                            "equation" : "x1 or (x2 and x3)"
                        }
                    },

                    "equation" : "(x1 or x2) and x3"
                }
            },
            "x3": "",
            
            "equation" : "(not x1 or x2) and x3"

        }
        
        ,
        "max_file_size" : 100000
    }
]