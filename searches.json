[
    {
        "name" : "behavior_trees",
        "keywords": [
            "ros",
            "ros2"
        ],
        "qualifiers":[
            "stars:>2", "language:python"
        ],

        "targets" : {
            "target_type" : "repo",
            "x1" : "github.*",
            "x2" : {

                "target_type" : "dir",
                "x1": ".*tests",
                "x2": ".*test",
                "x3": {
                    
                    "target_type" : "file",
                    "x1" : "CMakeLists.txt",
                    "x2" : ".*.py",
                    "x3" : {
                        
                        "target_type" : "code",
                        "x1" : "import py_trees",
                        "x2" : "from py_trees",
                        "equation" : "x1 or x2"
                    
                    },

                    "equation" : "x1 or (x2 and x3)"
                
                },

                "equation" : "(x1 or x2) and x3"
                
            },
            "x3": "bungus.*",
            
            "equation" : "(x1 and x2) or x3"
        }
    
        
        ,
        "max_file_size" : 100000
    }
]